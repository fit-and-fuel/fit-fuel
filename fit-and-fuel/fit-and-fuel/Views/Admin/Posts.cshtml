@model IEnumerable<Post>
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="container">
	<div class="card">
		<div class="card-header border-0">
			<h4 class="heading mb-0 blog-title">All Post</h4>
			
		</div>
		<div class="card-body">
			@foreach(var post in Model){
			<div class="third-post">
				<img src="~/img/s1.jpg" alt="">
				<div class="post-1">
					<div class="post-data">
						<span class="badge badge-secondary border-0 badge-sm">SEO Tools</span>
						<a href="javascript:void(0);">
							<h4>@post.Title</h4></a>
						<div class="mb-2">
							<img src="~/img//pic2.jpg" class="avatar rounded-circle me-2 small-post" alt="">
							<span><b class="text-primary">@post.nutritionist.Name</b> in @post.Time</span>
						</div>
						<span>
							@post.Description
						</span>
					</div>
				</div>
			</div>
			}
		</div>
	</div>

</div>
