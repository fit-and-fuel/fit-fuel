							@*
							here is the problem
							<div class="wpcf7 js">*@

							<div class="screen-reader-response"><p role="status" aria-live="polite" aria-atomic="true"></p> <ul></ul></div>
							<form asp-controller="User" asp-action="Register" method="post" class="wpcf7-form init" aria-label="Contact form" novalidate="novalidate" data-status="init">
								<div style="display: none;">
								</div>

								<div class="mb-3">
									<p>
										<label for="name">UserName</label><br>
										<span class="wpcf7-form-control-wrap" ><input size="40" class="wpcf7-form-control wpcf7-text wpcf7-validates-as-required" id="name" aria-required="true" aria-invalid="false" placeholder="UserName" type="text" name="Username"></span>
									</p>
								</div>
								<div class="mb-3">
									<p>
										<label for="password">Password</label><br>
										<span class="wpcf7-form-control-wrap" ><input size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email" id="password" aria-required="true" aria-invalid="false" placeholder="Password" type="password" name="Password"></span>
									</p>
								</div>
								<div class="mb-3">
									<p>
										<label for="email">Email</label><br>
										<span class="wpcf7-form-control-wrap"><input size="40" class="wpcf7-form-control wpcf7-text wpcf7-validates-as-required" id="email" aria-required="true" aria-invalid="false" placeholder="Email" type="text" name="Email"></span>
									</p>
								</div>
								<div class="mb-3">
									<p>
										<label for="password">Phone Number</label><br>
										<span class="wpcf7-form-control-wrap"><input size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email" id="password" aria-required="true" aria-invalid="false" placeholder="Password" type="text" name="PhoneNumber"></span>
									</p>
								</div>
								
								<div class="mb-3">
								<label for="Roles">Roles</label>
								<select id="Roles" name="Roles" multiple class="form-control">
									<option value="Admin">Admin</option>
									<option value="Nutritionist">Nutritionist</option>
									<option value="Patient">Patient</option>
								</select>
								</div>

								<div class="mb-0">
									<p>
										<input class="wpcf7-form-control has-spinner wpcf7-submit" type="submit" value="REGISTER"><span class="wpcf7-spinner"></span>
									</p>
								</div><div class="wpcf7-response-output" aria-hidden="true"></div>
							</form>
							<div asp-validation-summary="All"></div>
						</div>


					</div>
				</div>
			</div>
		</div>
	</div>
</section>



