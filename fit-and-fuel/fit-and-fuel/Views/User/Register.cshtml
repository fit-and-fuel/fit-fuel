<section class="elementor-section elementor-top-section elementor-element elementor-element-153008a elementor-section-content-bottom elementor-section-boxed elementor-section-height-default elementor-section-height-default" data-id="153008a" data-element_type="section">
	<div class="elementor-container elementor-column-gap-default">
		<div class="elementor-column elementor-col-50 elementor-top-column elementor-element elementor-element-ed8a95a" data-id="ed8a95a" data-element_type="column">
			<div class="elementor-widget-wrap elementor-element-populated">
				<div class="elementor-element elementor-element-ef5f742 elementor-widget elementor-widget-image" data-id="ef5f742" data-element_type="widget" data-widget_type="image.default">
					<div class="elementor-widget-container">
						<img decoding="async" loading="lazy" width="1242" height="1336" src="~/img/healthy.jpg" class="attachment-full size-full wp-image-2343" alt="" sizes="(max-width: 1242px) 100vw, 1242px">
					</div>
				</div>
			</div>
		</div>
		<div class="elementor-column elementor-col-50 elementor-top-column elementor-element elementor-element-aac29d5" data-id="aac29d5" data-element_type="column">
			<div class="elementor-widget-wrap elementor-element-populated">
				<div class="elementor-element elementor-element-4d11961 elementor-widget elementor-widget-pt-heading" data-id="4d11961" data-element_type="widget" data-settings="{&quot;decor&quot;:[]}" data-widget_type="pt-heading.default">
					<div class="elementor-widget-container">

						<div class="pt-heading">
							<h2 class="pt-heading-title h2">
								REGISTER
							</h2>
						</div>

					</div>
				</div>
				<div class="elementor-element elementor-element-a5d8048 elementor-widget elementor-widget-pt-contact-form-7" data-id="a5d8048" data-element_type="widget" data-widget_type="pt-contact-form-7.default">
					<div class="elementor-widget-container">

						<div class="pt-contact-form-7">
							@*
							here is the problem
							<div class="wpcf7 js">*@

							<div class="screen-reader-response"><p role="status" aria-live="polite" aria-atomic="true"></p> <ul></ul></div>
							<form asp-controller="User" asp-action="Register" method="post" class="wpcf7-form init" aria-label="Contact form" novalidate="novalidate" data-status="init">
								<div style="display: none;">
								</div>

								<div class="mb-3">
									<p>
										<label for="name">UserName</label><br>
										<span asp-validation-for="Username" class="wpcf7-form-control-wrap"></span>
											<input asp-for="Username" size="40" class="wpcf7-form-control wpcf7-text wpcf7-validates-as-required" id="name" aria-required="true" aria-invalid="false" placeholder="UserName" type="text" name="Username">
									</p>
								</div>
								<div class="mb-3">
									<p>
										<label for="password">Password</label><br>
										<span asp-validation-for="Password" class="wpcf7-form-control-wrap"></span>
										<input asp-for="Password" size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email" id="password" aria-required="true" aria-invalid="false" placeholder="Password" type="password" name="Password">
									</p>
								</div>
								<div class="mb-3">
									<p>
										<label for="email">Email</label><br>
										<span asp-validation-for="Email" class="wpcf7-form-control-wrap text-danger"></span>
										<input asp-for="Email" size="40" class="wpcf7-form-control wpcf7-text wpcf7-validates-as-required" id="email" aria-required="true" aria-invalid="false" placeholder="Email" type="text" name="Email">
									</p>
								</div>
								<div class="mb-3">
									<p>
										<label for="PhoneNumber">Phone Number</label><br>
										<span asp-validation-for="PhoneNumber" class="wpcf7-form-control-wrap"></span>
										<input asp-for="PhoneNumber" size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email" id="password" aria-required="true" aria-invalid="false" placeholder="Password" type="text" name="PhoneNumber">
									</p>
								</div>

								<div class="mb-3">
									<label for="Roles">Roles</label>
									<Select id="Roles" name="Roles"  class="form-control">
										<option value="Nutritionist">Nutritionist</option>
										<option value="Patient">Patient</option>
									</Select>
								</div>

								<div class="mb-0">
									<p>
										<input class="wpcf7-form-control has-spinner wpcf7-submit" type="submit" value="REGISTER"><span class="wpcf7-spinner"></span>
									</p>
								</div><div class="wpcf7-response-output" aria-hidden="true"></div>
							</form>
@* 							<div asp-validation-summary="All"></div>
 *@						</div>


					</div>
				</div>
			</div>
		</div>
	</div>
</section>



