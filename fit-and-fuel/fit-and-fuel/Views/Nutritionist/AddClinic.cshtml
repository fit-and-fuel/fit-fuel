@{
	Layout = "~/Views/Shared/_PageLayout.cshtml";
}

<div style="background-color:#141414 ;height:80px;margin-top:-10px;width:2000px;">
</div>

<form asp-controller="Nutritionist" asp-action="AddClinic" method="post" class="wpcf7-form init" aria-label="Contact form" novalidate="novalidate" data-status="init" enctype="multipart/form-data">
	<div style="display: none;">
	</div>

	<div class="mb-3">
		<p>
			<label for="Name">Name of Clinic</label><br>
			<span class="wpcf7-form-control-wrap">
				<input size="40" class="wpcf7-form-control wpcf7-text wpcf7-validates-as-required" id="name" aria-required="true" aria-invalid="false" placeholder="Name" type="text" name="Name">
			</span>
		</p>
	</div>
	@* 								<div class="mb-3">
	<p>
	<label for="PhoneNumber">phone number</label><br>
	<span class="wpcf7-form-control-wrap"><input size="40" class="wpcf7-form-control wpcf7-text wpcf7-validates-as-required" id="PhoneNumber" aria-required="true" aria-invalid="false" placeholder="PhoneNumber" type="text" name="PhoneNumber"></span>
	</p>
	</div> *@

	<div class="mb-3">
		<p>
			<label for="Address">Address</label><br>
			<span class="wpcf7-form-control-wrap"><input size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email" id="Address" aria-required="true" aria-invalid="false" placeholder="Address" type="text" name="Address"></span>
		</p>
	</div>

	<div class="mb-3">
		<p>
			<label for="PhoneNumber">PhoneNumber of Clinic</label><br>
			<span class="wpcf7-form-control-wrap"><input size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email" id="PhoneNumber" aria-required="true" aria-invalid="false" placeholder="PhoneNumber" type="text" name="PhoneNumber"></span>
		</p>
	</div>

	<div class="mb-0">
		<p>
			<input class="wpcf7-form-control has-spinner wpcf7-submit" type="submit" value="Add"><span class="wpcf7-spinner"></span>
		</p>
	</div><div class="wpcf7-response-output" aria-hidden="true"></div>
</form>
<div asp-validation-summary="All"></div>