@{
    Layout = "~/Views/Shared/_PageLayout.cshtml";
}

@model List<AvailableTime>



@foreach(var time in Model)
{
	<div class="m-5">
    <p>@time.DayOfWeek</p>
    <p>@time.Time</p>
    <br>
	</div>
}

<section class="elementor-section elementor-top-section elementor-element elementor-element-153008a elementor-section-content-bottom elementor-section-boxed elementor-section-height-default elementor-section-height-default" data-id="153008a" data-element_type="section">
	<div class="elementor-container elementor-column-gap-default">
		<div class="elementor-column elementor-col-50 elementor-top-column elementor-element elementor-element-aac29d5" data-id="aac29d5" data-element_type="column">
			<div class="elementor-widget-wrap elementor-element-populated">
				<div class="elementor-element elementor-element-4d11961 elementor-widget elementor-widget-pt-heading" data-id="4d11961" data-element_type="widget" data-settings="{&quot;decor&quot;:[]}" data-widget_type="pt-heading.default">
					<div class="elementor-widget-container">

						<div class="pt-heading">
							<h4 class="pt-heading-title h3">
								Your Profile
							</h4>
						</div>

					</div>
				</div>
				<div class="elementor-element elementor-element-a5d8048 elementor-widget elementor-widget-pt-contact-form-7" data-id="a5d8048" data-element_type="widget" data-widget_type="pt-contact-form-7.default">
					<div class="elementor-widget-container">

						<div class="pt-contact-form-7">
							@*
							here is the problem
							<div class="wpcf7 js">*@

							<div class="screen-reader-response"><p role="status" aria-live="polite" aria-atomic="true"></p> <ul></ul></div>
							<form asp-controller="Nutritionist" asp-action="AddAvailableTime" method="post" class="wpcf7-form init" aria-label="Contact form" novalidate="novalidate" data-status="init">
								<div style="display: none;">
								</div>
								<div class="mb-3">
									<label for="DayOfWeek">Day Of Week</label>
									<Select id="DayOfWeek" name="DayOfWeek" class="form-control">
										<option value="Sunday">Sunday</option>
										<option value="Monday">Monday</option>
										<option value="Tuesday">Tuesday</option>
										<option value="Wednesday">Wednesday</option>
										<option value="Thursday">Thursday</option>
										<option value="Friday">Friday</option>
										<option value="Saturday">Saturday</option>

									</Select>
								</div>
								<div class="mb-3">
									<p>
										<label for="Time">Time</label><br>
										<span class="wpcf7-form-control-wrap">
											<input size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email" id="Time" aria-required="true" aria-invalid="false" type="time" name="Time">
										</span>
									</p>
								</div>

								<div class="mb-0">
									<p>
										<input class="wpcf7-form-control has-spinner wpcf7-submit" type="submit" value="Add Time"><span class="wpcf7-spinner"></span>
									</p>
								</div><div class="wpcf7-response-output" aria-hidden="true"></div>
							</form>
							<div asp-validation-summary="All"></div>
						</div>


					</div>
				</div>
			</div>
		</div>
	</div>
</section>