@inject INutritionists nutritionists;
@inject IPatients patients;
@inject SignInManager<ApplicationUser> userManager;


@{
    var count = await notfication.CountNot();
    var notifications = await notfication.GetNotificationsForUser();
    var visibleNotifications = notifications.Take(3).ToList();

}

@{
    var nut = await nutritionists.GetMyProfile();

    var pat = await patients.GetMyProfile();
    var user = await userManager.UserManager.GetUserAsync(User);


}

@functions {
    // Function to format text with links as HTML anchor tags
    string FormatContentWithLinks(string content)
    {
        string pattern = @"(https?://\S+)|(www\.\S+)";
        var regex = new System.Text.RegularExpressions.Regex(pattern);

        return regex.Replace(content, match =>
        {
            string url = match.Value;
            return $"<a href='{url}' target='_blank'>Register</a>";
        });
    }
}

